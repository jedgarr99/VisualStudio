        <TextBox x:Name="txMonto" HorizontalAlignment="Left" Height="23" Margin="174,219,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btAlta" Content="Alta" HorizontalAlignment="Left" Margin="395,80,0,0" VerticalAlignment="Top" Width="75" Click="btAlta_Click"/>
        <Button x:Name="btSiguiente" Content="Siguiente" HorizontalAlignment="Left" Margin="395,115,0,0" VerticalAlignment="Top" Width="75" Click="btSiguiente_Click"/>

