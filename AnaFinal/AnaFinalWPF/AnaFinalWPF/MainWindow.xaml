        xmlns:local="clr-namespace:AnaFinalWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="600">
    <Grid>
        <TextBox x:Name="txClavePersona" HorizontalAlignment="Left" Height="23" Margin="174,250,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
